{{#if result.attempedBy user}}
    <div class="blue-grey darken-1">
        <div class="card-content white-text">
            <span class="card-title">Yo gave {{result.quizAttemped}} on {{result.timeAttempedAt}}</span>
            <p>Result : {{correctAnswers}} / {{totalQuestions}}</p>
            <p>Percentages: {{percentages}} %</p>
        </div>
        <div class="card-action">
            <a href="#">See Details</a>
        </div>
    </div>
{{/if}}
