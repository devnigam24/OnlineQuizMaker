<ul class="collapsible popout" data-collapsible="accordion">
    <li>
        <div class="collapsible-header card-panel blue-grey lighten-1">
            <div class="row">
                <div class="col s12 m12 white-text">
                    <div class="col s12 m12">
                        <span class="card-title">{{userText}} gave {{result.quizAttemped}} on {{result.timeAttempedAt}}</span>
                    </div>
                    <div class="col s6 m6">
                        <p>Result : {{correctAnswers}} / {{totalQuestions}}</p>
                    </div>
                    <div class="col s6 m6">
                        <p>Percentages: {{percentages}} %</p>
                    </div>
                </div>
            </div>
        </div>
        <div class="collapsible-body">
            <ul class="collapsible" data-collapsible="accordion">
                {{#each result.result as |oneResult|}}
                    <li>
                        {{#if oneResult.isCorrect}}
                        <div class="collapsible-header"><i class="material-icons icon-green">assignment_turned_in</i>{{oneResult.questionAnswered}}</div>
                          <div class="collapsible-body answer-true"><span>{{oneResult.answer}}</span></div>
                        {{else}}
                        <div class="collapsible-header"><i class="material-icons icon-pink">assignment_late</i>{{oneResult.questionAnswered}}</div>
                          <div class="collapsible-body answer-false"><span>{{oneResult.answer}}</span></div>
                        {{/if}}

                    </li>
                {{/each}}
            </ul>
        </div>
    </li>
</ul>
