{{#if result.attempedBy user}}
    <div class="blue-grey darken-1">
        <div class="card-content white-text">
            <span class="card-title">{{result.quizAttemped}}</span>
            <p>{{correctAnswers}} / {{totalQuestions}}</p>
        </div>
        <div class="card-action">
            <a href="#">See Details</a>
        </div>
    </div>
{{/if}}
