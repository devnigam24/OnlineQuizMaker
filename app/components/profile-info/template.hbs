<div class="card-image waves-effect waves-block waves-light">
    <img class="activator pofile-cover" src="assets/images/profile-cover.jpg">
</div>
<figure class="card-profile-image">
    <img src="assets/images/dev.png" alt="profile image" class="circle z-depth-2 responsive-img activator">
</figure>
<div class="card-content">
    <span class="card-title activator grey-text text-darken-4">{{sessionData.firstName}}
      <i class="material-icons right">more_vert</i></span>
    <p><a>{{sessionData.studentuniqueid}}</a></p>
</div>


<div class="card-reveal teal lighten-3">
    <span class="card-title grey-text text-darken-4">{{sessionData.firstName}} {{sessionData.lastName}}
      <i class="material-icons right">close</i></span>
    <h5>Student</h5>
    <p>{{sessionData.desc}}</p>
    <div class="divider"></div>
    <p><i class="material-icons left">email</i>{{sessionData.emailId}}</p>
    <p><i class="material-icons left">fingerprint</i>{{sessionData.studentuniqueid}}</p>
    <p><i class="material-icons left">local_phone</i>{{sessionData.phone}}</p>
    <p><i class="material-icons left">home</i>{{sessionData.streetAddress}}, {{sessionData.city}}, {{sessionData.pincode}}</p>
    <p><i class="material-icons left">location_on</i>{{sessionData.country}}</p>
</div>
