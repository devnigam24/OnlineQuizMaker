<h1>Log In to Continue</h1>
<ul class="errors">
    {{#each serverSideFormError as |error|}}
        <li><i class="material-icons">error</i>{{error.message}}</li>
    {{/each}}
</ul>
<form class="logInForm" method="post">
    User Name :{{input name="userName" type="email" class="validate"
  value=userName change=(action 'userNameValidation' userName)}}
    <br> Password :{{input name="Password" type="password" class="validate"
  value=Password change=(action 'passwordValidation' Password)}}
    <br>

    <button type="button" name="button" class="btn waves-effect waves-light" {{action "processLoginAction"}}>
          {{#if serverSideFormError.length 0}}
              Check
          {{else}}
              Join Now
          {{/if}}
           <i class="material-icons right">send</i>
    </button>

</form>
