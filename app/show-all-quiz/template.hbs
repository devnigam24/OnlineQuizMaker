{{#if model}}
    {{#each model.data as |model|}}
        {{all-quizzes-info model=model isStudent=this.isStudent
          checkAnswer=(action "checkAnswer" value=answerGiven)
          registerQuiz=(action "registerQuiz")
          submitQuiz=(action "submitQuiz")
        }}
    {{/each}}
{{else}}
    <div class="row">
        <div class="col s12 m5">
            <div class="card-panel teal">
                <span class="white-text"><h2>No Quizzes Available</h2></span>
            </div>
        </div>
    </div>
{{/if}}
